{
  "name": "Ice-Dev",
  "dockerComposeFile": "docker-compose.yml",
  "service": "ice",
  "workspaceFolder": "/workspaces/code",
  "customizations": {
    "vscode": {
      "settings": {
        "files.autoSave": "afterDelay",
        "files.autoSaveDelay": 1000
      },
      "extensions": [
		"ms-python.python",
		"ms-python.vscode-pylance",
		"esbenp.prettier-vscode",
		"github.copilot",
		"redhat.vscode-yaml",
		"ms-azuretools.vscode-docker",
		"bungcip.better-toml"
	]
    }
  }
}